{
  "name": "jobnik-manager",
  "version": "1.0.0-rc.118",
  "description": "This is jobnik management server",
  "main": "./src/index.ts",
  "scripts": {
    "test:unit": "vitest run --coverage.enabled=false --project unit",
    "test:integration": "vitest run --coverage.enabled=false --project integration",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prelint:fix": "npm run format:fix",
    "prelint": "npm run format",
    "lint:openapi": "openapi lint ./openapi3.yaml",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prebuild": "npm run clean && npm run migration:generate-types",
    "build": "tsc --project tsconfig.build.json && npm run prisma:copy && tsc-alias -p tsconfig.build.json && npm run assets:copy",
    "start": "npm run build && cd dist && node --import ./instrumentation.mjs ./index.js",
    "start:dev": "npm run build && cd dist && cross-env CONFIG_OFFLINE_MODE=true  node --enable-source-maps --import ./instrumentation.mjs ./index.js",
    "assets:copy": "copyfiles -f ./config/* ./dist/config && copyfiles -f ./openapi3.yaml ./dist/ && copyfiles ./package.json dist && copyfiles -f ./src/db/prisma/migrations/** ./dist/db/prisma/migrations",
    "prisma:copy": "node ./scripts/copyDirectories.mjs ./src/db/prisma/generated ./dist/db/prisma/generated",
    "clean": "rimraf dist",
    "generate:openapi-types": "openapi-helpers generate types ./openapi3.yaml ./src/openapi.d.ts --format --add-typed-request-handler",
    "generate:openapi-errors": "openapi-helpers generate errors ./openapi3.yaml ./src/common/generated/errors.ts --format --errors-output classes",
    "prepare": "node .husky/install.mjs",
    "migration:validate": "npx prisma validate --schema ./src/db/prisma/schema.prisma  ",
    "migration:format": "npx prisma format --check --schema ./src/db/prisma/schema.prisma  ",
    "migration:format-fix": "npx prisma format --schema ./src/db/prisma/schema.prisma  ",
    "migration:dev": "npx prisma migrate dev --schema ./src/db/prisma/schema.prisma",
    "migration:generate": "npx prisma migrate dev --schema ./src/db/prisma/schema.prisma --name ",
    "migration:generate-types": "npx prisma generate --schema ./src/db/prisma/schema.prisma",
    "migration:deploy": "npx prisma migrate deploy --schema ./src/db/prisma/schema.prisma",
    "openapi:build": "node scripts/generateVersionedOpenApi.mts"
  },
  "directories": {
    "test": "tests"
  },
  "author": "MapColonies",
  "license": "ISC",
  "dependencies": {
    "@godaddy/terminus": "^4.12.1",
    "@map-colonies/config": "^3.0.2",
    "@map-colonies/error-express-handler": "^3.0.0",
    "@map-colonies/express-access-log-middleware": "^3.0.1",
    "@map-colonies/js-logger": "^3.0.2",
    "@map-colonies/openapi-express-viewer": "^4.1.0",
    "@map-colonies/openapi-helpers": "^4.0.0",
    "@map-colonies/read-pkg": "^1.0.0",
    "@map-colonies/schemas": "^1.14.0",
    "@map-colonies/telemetry": "^10.1.0",
    "@opentelemetry/api": "^1.9.0",
    "@prisma/adapter-pg": "^6.19.0",
    "@prisma/client": "^6.19.0",
    "@prisma/instrumentation": "^6.19.0",
    "compression": "^1.8.1",
    "date-fns": "^4.1.0",
    "express": "^4.21.2",
    "express-openapi-validator": "^5.6.0",
    "http-status-codes": "^2.3.0",
    "lodash": "^4.17.21",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
    "xstate": "^5.24.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@faker-js/faker": "^9.9.0",
    "@map-colonies/commitlint-config": "^1.1.1",
    "@map-colonies/eslint-config": "^6.0.0",
    "@map-colonies/infra-copilot-instructions": "^1.2.0",
    "@map-colonies/prettier-config": "^0.0.1",
    "@map-colonies/tsconfig": "^1.0.1",
    "@opentelemetry/sdk-trace-node": "^1.30.1",
    "@redocly/cli": "^2.12.4",
    "@types/compression": "^1.7.5",
    "@types/config": "^3.3.5",
    "@types/date-fns": "^2.5.3",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.17.21",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.0",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.8",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "copyfiles": "^2.4.1",
    "cross-env": "^10.1.0",
    "docker-compose": "^1.3.0",
    "eslint": "^9.39.1",
    "get-port-please": "^3.2.0",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "jest-create-mock-instance": "^2.0.0",
    "jest-extended": "^7.0.0",
    "jest-openapi": "^0.14.2",
    "openapi-merge-cli": "^1.3.2",
    "openapi-types": "^12.1.3",
    "prettier": "^3.7.4",
    "pretty-quick": "^4.1.1",
    "prisma": "^6.19.0",
    "prisma-json-types-generator": "^3.6.2",
    "rimraf": "^6.1.2",
    "supertest": "^7.1.4",
    "tsc-alias": "^1.8.11",
    "type-fest": "^4.38.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "vitest-mock-extended": "^3.1.0",
    "yaml": "^2.8.2",
    "zx": "^8.8.5"
  }
}
